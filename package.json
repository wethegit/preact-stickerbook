{
  "name": "@wethegit/preact-stickerbook",
  "version": "0.0.8",
  "description": "Easily create collage apps that are accessible by default.",
  "source": "src/index.js",
  "main": "dist/preact-stickerbook.js",
  "module": "dist/preact-stickerbook.module.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "browser": "./dist/preact-stickerbook.module.js",
      "import": "./dist/preact-stickerbook.modern.js",
      "require": "./dist/preact-stickerbook.js"
    },
    "./styles.css": "./dist/preact-stickerbook.css"
  },
  "scripts": {
    "build": "rm -rf dist; microbundle build -f modern,es,cjs src/index.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wethegit/preact-stickerbook.git"
  },
  "keywords": [
    "preact",
    "preact-component"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wethegit/preact-stickerbook/issues"
  },
  "homepage": "https://github.com/wethegit/preact-stickerbook#readme",
  "peerDependencies": {
    "preact": "^10.5.12"
  },
  "devDependencies": {
    "microbundle": "^0.13.0"
  },
  "dependencies": {
    "wtc-math": "^0.0.4"
  }
}
