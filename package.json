{
  "name": "@wethegit/preact-stickerbook",
  "amdName": "WTCPreactStickerbook",
  "version": "2.0.0-beta.15",
  "description": "Easily create collage apps that are accessible by default.",
  "source": "./src/index.js",
  "main": "./dist/main.js",
  "css": "./dist/style.css",
  "targets": {
    "main": {
      "source": "src/index.js"
    },
    "css": {
      "source": "src/index.scss"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wethegit/preact-stickerbook.git"
  },
  "keywords": [
    "preact",
    "preact-component"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "parcel build && parcel build src/sticker.scss src/stickerbook.scss"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wethegit/preact-stickerbook/issues"
  },
  "homepage": "https://github.com/wethegit/preact-stickerbook#readme",
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat"
  },
  "dependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/preset-react": "^7.18.6",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "parcel": "^2.8.3",
    "preact": "^10.13.1",
    "wtc-math": "^1.0.17",
    "wtc-simplegl": "^0.0.4"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "sass": "^1.59.3"
  }
}
